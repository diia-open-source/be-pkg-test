{
    "name": "@diia-inhouse/test",
    "version": "7.2.0",
    "description": "test-utils",
    "main": "dist/index.js",
    "author": "Diia",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "tsc",
        "prebuild": "rimraf dist",
        "prepare": "npm run build",
        "semantic-release": "semantic-release",
        "start": "npm run build && node dist/index.js",
        "lint": "eslint . && prettier --check .",
        "lint-fix": "eslint . --fix && prettier --write .",
        "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts registry.npmjs.org --validate-https",
        "find-circulars": "madge --circular --extensions ts ./"
    },
    "files": [
        "dist",
        "vitest"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/types/index.d.ts"
        },
        "./vitest": {
            "default": "./vitest/extend-expect.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/types/index.d.ts"
            ]
        }
    },
    "engines": {
        "node": ">=22"
    },
    "keywords": [],
    "dependencies": {
        "@diia-inhouse/design-system": "3.0.1",
        "@vitest/expect": "4.0.16",
        "glob": "10.5.0",
        "lodash": "4.17.23",
        "luxon": "3.7.2",
        "nock": "13.5.6",
        "superstruct": "2.0.2",
        "type-fest": "4.41.0",
        "vitest": "4.0.16"
    },
    "devDependencies": {
        "@diia-inhouse/configs": "6.1.1",
        "@diia-inhouse/eslint-config": "8.2.4",
        "@diia-inhouse/types": "11.3.0",
        "@types/lodash": "4.17.21",
        "@types/luxon": "3.7.1",
        "bson": "6.10.4",
        "drizzle-orm": "0.45.1",
        "lockfile-lint": "4.14.1",
        "madge": "8.0.0",
        "mongoose": "8.20.4",
        "rimraf": "6.1.2",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "@diia-inhouse/types": ">=10.0.11",
        "bson": ">=6.2.0",
        "drizzle-orm": ">=0.38.2",
        "mongoose": ">=8.0.0"
    },
    "peerDependenciesMeta": {
        "drizzle-orm": {
            "optional": true
        },
        "mongoose": {
            "optional": true
        }
    },
    "resolutions": {
        "@babel/traverse": "7.25.9"
    },
    "commitlint": {
        "extends": "@diia-inhouse/configs/dist/commitlint"
    },
    "release": {
        "extends": "@diia-inhouse/configs/dist/semantic-release/package",
        "branches": [
            "main"
        ]
    },
    "prettier": "@diia-inhouse/eslint-config/prettier",
    "madge": {
        "tsConfig": "./tsconfig.json"
    },
    "repository": "https://github.com/diia-open-source/be-pkg-test.git",
    "license": "SEE LICENCE IN LICENCE.md"
}
